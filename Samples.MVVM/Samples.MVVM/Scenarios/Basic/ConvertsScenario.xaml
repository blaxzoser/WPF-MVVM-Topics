<Window x:Class="Samples.MVVM.Scenarios.Basic.ConvertsScenario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Samples.MVVM.Scenarios.Basic"
        xmlns:View="clr-namespace:Samples.MVVM.View"
        mc:Ignorable="d"
        Title="ConvertsScenario" Height="450" Width="800">
    <Grid>
        <View:CustomerAddView HorizontalAlignment="Left" Height="405" Margin="27,9,0,0" VerticalAlignment="Top" Width="256" RenderTransformOrigin="0.5,0.5">
            <View:CustomerAddView.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-1.208"/>
                    <RotateTransform/>
                    <TranslateTransform X="-1.992"/>
                </TransformGroup>
            </View:CustomerAddView.RenderTransform>
        </View:CustomerAddView>
    </Grid>
</Window>
